{
  "cli": {
    "version": ">= 16.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "pva@techtech.in",
        "ascAppId": "6749745342",
        "appleTeamId": "2R7AXFA37K"
      }
    },
    "production-new": {
      "android": {
        "serviceAccountKeyPath": "./path-to-your-service-account.json",
        "track": "production"
      },
      "ios": {
        "appleId": "pva@techtech.in",
        "ascAppId": "6749745342",
        "appleTeamId": "2R7AXFA37K"
      }
    },
    "development": {
      "ios": {
        "appleId": "pva@techtech.in",
        "ascAppId": "6749745342",
        "appleTeamId": "2R7AXFA37K"
      }
    }
  }
}
