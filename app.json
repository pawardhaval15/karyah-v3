{
  "expo": {
    "name": "Karyah:",
    "slug": "karyah",
    "version": "2.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "hooks": {
      "postPublish": []
    },
    "splash": {
      "image": "./assets/Slice 1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": false,
      "bundleIdentifier": "in.techtech.karyah",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to allow photo selection and uploads.",
        "NSCameraUsageDescription": "This app requires access to the camera to allow photo and video capture.",
        "NSMicrophoneUsageDescription": "This app requires access to the microphone for recording audio.",
        "NSPhotoLibraryAddUsageDescription": "This app requires permission to save photos and videos to your library.",
        "NSDocumentsFolderUsageDescription": "This app requires access to your documents for file uploads and access.",
        "NSUserNotificationUsageDescription": "Karyah needs permission to send you important alerts and updates.",
        "UIBackgroundModes": ["fetch", "remote-notification"]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.developer.usernotifications.communication": true
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.techtech.karyah",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "buildToolsVersion": "35.0.0",
      "versionCode": 13,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#366CD9"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font",
      "expo-audio",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "versionCode": 13,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android": {
            "enableMultiDex": true
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "android": {
            "enableMultiDex": true
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "platforms": ["ios", "android", "web"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "41284ef7-67c7-4c81-a0c9-f0a86571c7b2"
      }
    },
    "owner": "piyushkatole16"
  }
}
